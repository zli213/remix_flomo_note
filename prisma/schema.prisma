generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = "file: .. /database.db"
}

 model Note {
  id String @id @default(uuid())
  content String
  createAt DateTime @default(now()) @map("create_at") 
  tags Tag[]
}
model Tag {
  title String @id
  notes Note[]
}
model User {
  id String @id @default(uuid())
  username String @unique
  password String
  createAt DateTime @default(now()) @map("create_at") 
}